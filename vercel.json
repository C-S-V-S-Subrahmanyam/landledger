{
  "version": 2,
  "name": "landledger",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_APP_NETWORK": "testnet"
  },
  "functions": {
    "frontend/**/*.{js,ts,tsx}": {
      "includeFiles": "frontend/**"
    }
  }
}
